# Copy to .env and set values. Used by populate_*.py and by the app when using these DBs.
# PostgreSQL (must match app ConnectionStrings:DefaultConnection when using Postgres)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=exampleproject
POSTGRES_PASSWORD=exampleproject
POSTGRES_DB=exampleproject

# MongoDB (must match app Mongo:* settings when using MongoDB)
MONGO_URI=mongodb://localhost:27017
MONGO_DB=exampleproject
MONGO_AUDIT_COLLECTION=audit
